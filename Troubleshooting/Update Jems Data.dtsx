                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
            
            emp.EmpCode+''+'@jdg.co.za' as userPrincipleName,
            case when isnull(emp.KnownAs,'') = '' then isnull(emp.FirstName,'') else isnull(emp.KnownAs,'') end+' '+isnull(emp.LastName,'') as 'displayName',
            emp.EmpCode as sAMAccountName,
            cast(case when   emp.Email IS null then (select top 1 [mail] FROM [jdx_iintegrate].[dbo].[ds_ad_user] where employeeID = emp.EmpCode and [mail]  is not null)  else emp.Email end as varchar(60)) as 'email',
            --cast((select top 1 [mail] FROM [jdx_iintegrate].[dbo].[ds_ad_user] where employeeID = emp.EmpCode and [mail]  is not null) as varchar(60)) as 'email',
           case when map.ds_status = 'Deleted' and emp.LeaveDate &gt; emp.HireDate then 'Deleted'  else StatusEmpCode end as 'status',
            case when emp.LeaveDate &lt; GETDATE() then emp.LeaveDate else NULL end as 'ActualLeaveDate',
            emp.HireDate as 'startdate',
            '' as 'CompanyOps',
            pos.Chain as 'Company',
            pos.BusinessUnit as 'SiteName',---pos.CostCentreDescription as 'SiteName',
            pos.Division,
            pos.Area,
            pos.BusinessUnitCode,
            pos.Title as 'Job Title',
            pos.OperatingDivision as 'OppsDivision',
            pos.GradeCode as 'JobGrade',
            pos.[OccupationCode],
            pos.[SAPCostCentre],
            case when pos.[Cluster] = 'Cluster 1' then 'Cluster One' when pos.[Cluster] = 'Cluster 2' then 'Cluster Two' when pos.[Cluster] = 'Cluster 3' then 'Cluster Three' else pos.[Cluster] end as [Cluster],
           -- (select top 1 [GlobalEmpCode] from [jdx_iintegrate].[dbo].[ds_jems_position_map] with (nolock) where GlobalPosCode = pos.ReportsToGlobalPosCode and ds_status not in ('deleted') ) as ReportsTo

isnull((select top 1 [GlobalEmpCode] from [jdx_iintegrate].[dbo].[ds_jems_position_map] as #Map with (nolock) where #Map.GlobalPosCode = pos.ReportsToGlobalPosCode and #Map.[ds_status] &lt;&gt; 'Deleted'),(select top 1 #Map.[GlobalEmpCode] from [jdx_iintegrate].[dbo].[ds_jems_position_map] as #Map with (nolock) where  #Map.[ds_status] &lt;&gt; 'Deleted' and #Map.GlobalPosCode = (select top 1 ReportsToGlobalPosCode from dbo.ds_jems_position as #Position with (nolock) where #Position.[PosCode] = pos.ReportsToGlobalPosCode and #Position.[StatusPosCode] = 'Active' ) )) as ReportsTo


   ,'' as 'TargetAudienceGrouping'
   ,(select top 1 [sAMAccountName] FROM [jdx_iintegrate].[dbo].[stage_ad_user] with (nolock) where [employeeID] = emp.EmpCode) as ADsAMAccountName
,map.DateAppointed
,emp.BirthDate
,emp.GenderCode
,emp.EthnicCode
      FROM
            dbo.ds_jems_employee as emp with (nolock)
            inner join dbo.ds_jems_position_map as map with (nolock)
            on emp.EmpCode = map.GlobalEmpCode
            inner join dbo.ds_jems_position as pos with (nolock)
            on map.GlobalPosCode = pos.PosCode

--where emp.EmpCode = '204820'

  order by  pos.BusinessUnitCode</property>
