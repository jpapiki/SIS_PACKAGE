            <component
              refId="Package\Get Price Ticket Data\Source Store Selling Data"
              componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Source Store Selling Data"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[Admin].[Userr]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
		 CONVERT(varchar,CONVERT(numeric,REPLACE(REPLACE(mara.[MATNR], ';', ''), 'SINGLE', ''))) AS [SourceSystemArticleCode]
		,MAKT.[MAKTX] AS 'ArticleDescription'
		,mvke.VKORG AS [SalesOrganisationCode]
		,tvkot.VTEXT AS 'SalesOrganisationDescription'
		,t1.MMSTA AS [ArticleStatus]
		,t.[Sales Price inc. Tax] AS [SellingPriceIncl]
		,CONVERT(numeric(10,2),t.[Sales Price inc. Tax] / 1.14) AS [SellingPriceExcl]
		,t.KONWA AS [SellingCurrency]
		,CASE WHEN CONVERT(numeric(10,2),t.[Sales Price inc. Tax] / 1.14) IS NULL 
			 THEN NULL 
			 ELSE t.[Valid From] 
			 END AS [SellingStartDate]
		,CASE WHEN CONVERT(numeric(10,2),t.[Sales Price inc. Tax] / 1.14) IS NULL 
			 THEN NULL 
			 ELSE t.[Valid To] 
			 END AS [SellingEndDate]
		,'Selling' AS 'Type'
		,mara.[ZZFB01] AS 'FAB1'
		,mara.[ZZFB02] AS 'FAB2'
		,mara.[ZZFB03] AS 'FAB3'
		,mara.[ZZFB04] AS 'FAB4'
		,mara.[ZZFB05] AS 'FAB5'
		,mara.[ZZFB06] AS 'FAB6'
		,mara.[ZZFB07] AS 'FAB7'
		,mara.[ZZFB08] AS 'FAB8'
--INTO #TempSellingStore
FROM [DSData].[dbo].[MARA_ARTICLEMASTER] AS mara WITH(NOLOCK) 
LEFT OUTER JOIN  [DSData].[dbo].[MVKE_SALESARTICLE] AS mvke WITH(NOLOCK) 
ON mara.MATNR = mvke.MATNR 
LEFT OUTER JOIN (  SELECT * 
					 FROM (	SELECT CASE  a071.[KSCHL] WHEN 'VKP0' 
														THEN 'Sales Price inc. Tax' 
													  WHEN 'VKA0' 
														THEN 'Promo Price inc. Tax' 
													  ELSE a071.[KSCHL] 
													  END AS [KSCHL]
										,a071.[VKORG] AS 'Sales Org'
										,a071.[VTWEG] AS DistributionChannel
										,a071.[MATNR] AS 'Article Number'
										,a071.[VRKME] AS UOM
										,a071.[DATAB] AS 'Valid From'
										,a071.[DATBI] AS 'Valid To'
										,konp.KBETR AS Amount
										,konp.[AKTNR] AS Promo
										,konp.KONWA
										,konp.[KNUMH]
										,konp.LIFNR
										,a071.VTWEG
							FROM [DSData].[dbo].[A071] AS a071 WITH(NOLOCK) 
							LEFT OUTER JOIN  [DSData].[dbo].[KONP] AS konp WITH(NOLOCK)
							ON a071.KNUMH = konp.KNUMH AND a071.KSCHL = konp.KSCHL
							WHERE a071.KSCHL in ('VKP0') AND YEAR(a071.[DATBI]) = '9999'
						) t
					pivot
					(
						MIN(Amount) FOR [KSCHL]
						in ([Sales Price inc. Tax],[Promo Price inc. Tax])
					) AS pvt
				) AS t 
ON t.[Article Number] = mvke.MATNR AND t.[Sales Org] = mvke.VKORG 
LEFT OUTER JOIN   (
						SELECT DISTINCT  marc.MATNR AS MATNR
										,t001w.VKORG AS VKORG
										,marc.MMSTA AS MMSTA
										,marc.MMSTD AS MMSTD
										,t001w.EKORG 
						FROM [DSData].[dbo].[MARC_SITEARTICLE] AS marc WITH(NOLOCK) 
						LEFT OUTER JOIN [DSData].[dbo].[T001W_SITES] AS t001w WITH(NOLOCK) 
						ON marc.[WERKS] like 'Z0%' AND marc.WERKS = t001w.WERKS
				  ) AS t1 
ON t1.MATNR = mvke.MATNR AND t1.VKORG = mvke.VKORG AND t1.MMSTD &gt;= t.[Valid From] 
LEFT OUTER JOIN [DSData].[dbo].[TVKOT_SALESORGANIZATIONS] tvkot WITH(NOLOCK)
ON tvkot.VKORG = mvke.VKORG 
LEFT OUTER JOIN [DSData].[dbo].[MAKT_ARTICLEDESCRIPTIONS] makt WITH(NOLOCK) 
ON makt.MATNR = mara.[MATNR]
WHERE CASE WHEN CONVERT(numeric(10,2),t.[Sales Price inc. Tax] / 1.14) IS NULL 
		   THEN NULL 
		   ELSE t.[Valid To] 
		   END IS NOT NULL 
--AND (mvke.VKORG in ('SO20') or tvkot.VTEXT like '%connection%')
AND (tvkot.VTEXT != 'JD Supply Chain Serv' AND tvkot.VTEXT != 'JD SC Clearance' AND tvkot.VTEXT != 'Central Purch Corp')
ORDER BY tvkot.VTEXT</property>
