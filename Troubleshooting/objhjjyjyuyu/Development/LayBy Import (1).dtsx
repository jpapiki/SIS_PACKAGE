              SQLTask:SqlStatementSource="DECLARE @hid INT, @Ref varchar(max), @tab char(1) = CHAR(9), @q varchar(max), @Sub varchar(max), @fn varchar(max)&#xA;&#xA;set @hid = (Select count(distinct [Site Code]) FROM [Every_Angle].[dbo].[vw_LayBy] with (nolock))&#xA;&#xA;create Table #Temp1&#xA;(&#xA;ref varchar(max) null&#xA;)&#xA;&#xA;--select @hid&#xA;&#xA;WHILE @hid &gt; 0&#xA;&#xA;BEGIN &#xA;set nocount on&#xA;&#xA;set @Ref = (Select distinct top 1 [Site Code] FROM [Every_Angle].[dbo].[vw_LayBy] with (nolock) where [Site Code] not like '%L%' and [Site Code] not in (Select distinct ref from #temp1))&#xA;&#xA;set @q = 'Select distinct * FROM [Every_Angle].[dbo].[vw_LayBy] with (nolock) where [Site Code] = ''' + @Ref + ''''&#xA;set @fn = @Ref + 'Lay-By Collections.csv'&#xA;set @Sub = @Ref + ' - LayBy Report'&#xA;&#xA;EXEC msdb.dbo.sp_send_dbmail&#xA;@profile_name = 'SQL Data',&#xA;@recipients = 'SiteReports@sps.jdg.co.za',&#xA;@subject = 'Lay-By',--@Ref,--@Sub,&#xA;@query = @q,&#xA;@attach_query_result_as_file = 1,&#xA;@query_attachment_filename = @fn,--'SiteDetails.csv',&#xA;@query_result_separator=@tab,&#xA;@query_result_no_padding=1,&#xA;@append_query_error = 0,&#xA;@query_result_header = 1&#xA;&#xA;SET @hid = @hid - 1;&#xA;&#xA;Insert into #Temp1&#xA;(&#xA;ref&#xA;)&#xA;Values&#xA;(&#xA;@ref&#xA;)&#xA;&#xA;set nocount off&#xA;END&#xA;&#xA;--Select * From #Temp1&#xA;--Select @hid&#xA;&#xA;drop table #Temp1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
